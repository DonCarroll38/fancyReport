        }

        function CDExpandCollapseButton() {
            var Section = 'CDBodyGoesHere';
            var i = 0;
            var RowCnt = 0;
            var rowHeight = 0;
            var TotalRowHeight = 0;
            var CDHeight = 0;
            var RptExpandCollapse = document.getElementById('CDExpandCollapse')

            CDHeight = document.getElementById('CD_RowHeaders').clientHeight + 20; //Plus 10 for padding top and 10 for padding bottom

            for (i = 0; i < CrashDumps.CrashDumpRecords.length; i++) {
                if (CrashDumps.CrashDumpRecords[i].Name != ' ' && CrashDumps.CrashDumpRecords[i].Name != '') {
                    rowHeight = 0;
                    rowHeight = document.getElementById('CD_RHShown0' + RowCnt).clientHeight > rowHeight ? document.getElementById('CD_RHShown0' + RowCnt).clientHeight : rowHeight; //This height check is done here because it is the cell that can make the whole row grow.
                    rowHeight = document.getElementById('CD_RHShown1' + RowCnt).clientHeight > rowHeight ? document.getElementById('CD_RHShown1' + RowCnt).clientHeight : rowHeight; //This height check is done here because it is the cell that can make the whole row grow.
                    rowHeight = document.getElementById('CD_RHShown2' + RowCnt).clientHeight > rowHeight ? document.getElementById('CD_RHShown2' + RowCnt).clientHeight : rowHeight; //This height check is done here because it is the cell that can make the whole row grow.
                    rowHeight = document.getElementById('CD_RHShown3' + RowCnt).clientHeight > rowHeight ? document.getElementById('CD_RHShown3' + RowCnt).clientHeight : rowHeight; //This height check is done here because it is the cell that can make the whole row grow.
                    //CDHeight += rowHeight + 11;  //The extra 10 is 5px padding for bottom and top which is set in the "RowHeader" css class.
                    //CDHeight += Number(document.getElementById('CD_RH' + i).getAttribute('rowHeight'));
                    //document.getElementById('CD_RH' + RowCnt).style.height = document.getElementById('CD_RH' + RowCnt).getAttribute('rowHeight') + 'px';
                    document.getElementById('CD_RH' + RowCnt).style.height = rowHeight + 'px';

                    TotalRowHeight += rowHeight;
                    RowCnt++;
                }
            }

            //CDHeight = Number(document.getElementById(Section).getAttribute('rowHeight')) + 57 + 10 + 44 + 17;  //The 57 includes the header row below the message.  The 10 is for the last border line before the footer.  The 44 is the entire footer height.
            CDHeight = TotalRowHeight + 57 + 10 + 44 + 25;  //The 57 includes the header row below the message.  The 10 is for the last border line before the footer.  The 44 is the entire footer height.

            if (RptExpandCollapse.innerHTML == 'Show&nbsp;&nbsp;') {
                document.getElementById(Section).style.height = CDHeight + 'px';
                document.getElementById(Section).style.padding = '10px';
                setTimeout(function () { RptExpandCollapse.innerHTML = 'Hide&nbsp;&nbsp;'; }, 1200);
                setTimeout(function () { document.getElementById('CDExpandCollapsePic').src = 'data:image/png;base64,'; }, 1200);

                setTimeout(function () { RemoveStyleAttrFromDiv(Section) }, 1300);
            } else {
                document.getElementById(Section).style.height = CDHeight + 'px';
                setTimeout(function () { CDSetHeightAttr(Section) }, 0100);
            }
        }

        function CDSetHeightAttr(DivName) {
            document.getElementById(DivName).style.padding = '0px';
            document.getElementById(DivName).style.overflow = 'hidden';
            document.getElementById(DivName).style.height = '0px';

            setTimeout(function () { document.getElementById('CDExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;'; }, 1200);
            setTimeout(function () { document.getElementById('CDExpandCollapsePic').src = 'data:image/png;base64,'; }, 1200);
        }

        function WUSetHeightAttr(DivName) {
            document.getElementById(DivName).style.padding = '0px';
            document.getElementById(DivName).style.overflow = 'hidden';
            document.getElementById(DivName).style.height = '0px';

            document.getElementById('WUExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;';
            document.getElementById('WUExpandCollapsePic').src = 'data:image/png;base64,';
        }

        function RemoveStyleAttrFromDiv(DivName) {
            document.getElementById(DivName).removeAttribute('style');
            document.getElementById(DivName).style.padding = '10px';
        }

        function SectionHeader(count, Section) {
            var strHTML = '';
            var SectButton = "";
            var SectTitle = "";
            
            switch(Section)
            {
                case "BIOS":
                    SectButton = "BIOSExpandCollapseButton()";
                    SectTitle = "BIOS Settings";
                    break;
                case "WU":
                    SectButton = "WUExpandCollapseButton()";
                    SectTitle = "Windows Updates";
                    break;
                case "DAndSP":
                    SectButton = "ExpandAllRecs(&apos;DAndSP&apos;)";
                    SectTitle = "Drivers and Softpaqs";
                    break;
                case "HDD":
                    SectButton = "ExpandAllRecs(&apos;HDD&apos;)"
                    SectTitle = "Drive Information";
                    break;
                case "Eng":
                    SectButton = "EngExpandCollapseButton()"
                    SectTitle = "Engineering Data";
                    break;
                case "CD":
                    SectButton = "CDExpandCollapseButton()"
                    SectTitle = "Crash Dumps";
                    break;
                case "BIOSBottom":
                    SectButton = "BIOSExpandCollapseButton()";
                    SectTitle = "BIOS Settings";
                    break;
            }

            if (count > 0) {
                //if (SectButton == "ExpandAllRecs(DAndSP)") {
                    strHTML = '<div style="font-weight:bolder;color:rgb(83, 84, 85);font-size:large;" class="w3-col s6 m8 l10">' + SectTitle + '</div>';
                    strHTML = strHTML + '<div class="w3-col s6 m4 l2 RptSectionExpandCollapseButton" style="position:relative;top:5px;" onclick="' + SectButton + ' ; return false;">';
                    strHTML = strHTML + '<a href="#">';
                    strHTML = strHTML + '<div id="' + Section + 'ExpandButtonHighlighting" class="w3-col s12 m12 l12 ButtonHighlighting">';
                    strHTML = strHTML + '<div id="' + Section + 'ExpandCollapseContainer" class="w3-col s1 m2 l2">';
                    strHTML = strHTML + '<img id="' + Section + 'ExpandCollapsePic" style="height:20px;" src="data:image/png;base64," />';
                    strHTML = strHTML + '</div>';
                    strHTML = strHTML + '<div id="' + Section + 'ExpandCollapse" class="w3-col s11 m10 l10 HoverShowBox">Show&nbsp;&nbsp;</div>';
                    strHTML = strHTML + '</div>';
                    strHTML = strHTML + '</a>';
                    strHTML = strHTML + '</div>';
                //} else {
                //    
                //}
            } else {
                strHTML = '<div style="font-weight:bolder;color:rgb(83, 84, 85);font-size:large;" class="w3-col s12 m12 l12">' + SectTitle + '</div>';
            }

            document.getElementById(Section + 'SectionHeader').innerHTML = strHTML;
        }

        function CalculateHeightOnEachExpandRow(Section) {
            var intHeight = 0;

            switch (Section) {
                case "DAndSP":
                    CalculateHeightOnEAchExpandRowLoops(DAndSP.DAndSPRecords, Section)
                    break;
                case "HDD":
                    CalculateHeightOnEAchExpandRowLoops(HDD.HDDRecords, Section)
                    break;
            }
        }

        function CalculateHeightOnEAchExpandRowLoops(JSonObject, Section) {
            for (i = 0; i < JSonObject.length; i++) {
                if (document.getElementById(Section + 'PropsRowsStartHere' + i) != null) {
                    intHeight = 0;
                    intHeight = document.getElementById(Section + 'PropsRowsStartHere' + i).clientHeight + 'px';
                    document.getElementById(Section + 'PropsRowsStartHere' + i).setAttribute('rowHeight', intHeight);
                    document.getElementById(Section + 'PropsRowsStartHere' + i).className = 'ExtraContentForDAndSP';
                }
            }
        }

        function Pin_UnPin(i, Section) {
            var bolMatch = true;
            var j = 0;

            if (Section == "DAndSP") {
                var ExtraContentForDAndSP = document.getElementById(Section + 'PropsRowsStartHere' + i);
                ExtraContentForDAndSP.style.WebkitTransitionDelay = '0.1s';
                ExtraContentForDAndSP.style.transitionDelay = '0.1s';

                if (DAndSP.DAndSPRecords[i].Pinned == 'Yes') {
                    DAndSP.DAndSPRecords[i].Pinned = 'No';
                    setTimeout(function () { document.getElementById('DAndSPPM' + i).innerHTML = '+'; }, 0900);
                    ExtraContentForDAndSP.removeAttribute('style');
                    ExtraContentForDAndSP.style.height = 0;
                } else {
                    DAndSP.DAndSPRecords[i].Pinned = 'Yes';
                    setTimeout(function () { document.getElementById('DAndSPPM' + i).innerHTML = '-'; }, 0900);
                    ExtraContentForDAndSP.removeAttribute('style');
                    ExtraContentForDAndSP.style.height = ExtraContentForDAndSP.getAttribute('rowheight');
                }
        
                //This section below handles whether or not the "Show" button will say "Show" or "Hide".
                for (j = 0; j < DAndSP.DAndSPRecords.length; j++) {
                    if (DAndSP.DAndSPRecords[j].Pinned == 'No')
                        bolMatch = false;
                }
            } else {
                var ExtraContentForDAndSP = document.getElementById(Section + 'PropsRowsStartHere' + i);
                ExtraContentForDAndSP.style.WebkitTransitionDelay = '0.1s';
                ExtraContentForDAndSP.style.transitionDelay = '0.1s';

                if (HDD.HDDRecords[i].Pinned == 'Yes') {
                    HDD.HDDRecords[i].Pinned = 'No';
                    setTimeout(function () { document.getElementById('HDDPM' + i).innerHTML = '+'; }, 0900);
                    ExtraContentForDAndSP.removeAttribute('style');
                    ExtraContentForDAndSP.style.height = 0;
                } else {
                    HDD.HDDRecords[i].Pinned = 'Yes';
                    setTimeout(function () { document.getElementById('HDDPM' + i).innerHTML = '-'; }, 0900);
                    ExtraContentForDAndSP.removeAttribute('style');
                    ExtraContentForDAndSP.style.height = ExtraContentForDAndSP.getAttribute('rowheight');
                }

                //This section below handles whether or not the "Show" button will say "Show" or "Hide".
                for (j = 0; j < HDD.HDDRecords.length; j++) {
                    if (HDD.HDDRecords[j].Pinned == 'No')
                        bolMatch = false;
                }
            }

            //If at least one of the driver rows has an unpinned row then show the Show button
            if (bolMatch == false) {
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;'; }, 0900);
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapsePic').src = 'data:image/png;base64,'; }, 0900);
                
                if (Section == "DAndSP") { DAndSP.Expanded = 'No'; } else { HDD.Expanded = 'No'}
            } else {
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapse').innerHTML = 'Hide&nbsp;&nbsp;'; }, 0900);
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapsePic').src = 'data:image/png;base64,'; }, 0900);
                if (Section == "DAndSP") { DAndSP.Expanded = 'Yes'; } else { HDD.Expanded = 'Yes' }
            }
        }

        function SetRowHeaderforContentMouseOver(i, Section) {
            var RowHeader = "";
            var Plus_Minus = "";

            if (Section == "DAndSP") {
                RowHeader = document.getElementById('DriversAndSoftpaqsTitle' + i);
                Plus_Minus = document.getElementById('PinnedIconDAndSP' + i);
            } else if (Section == "HDD") {
                RowHeader = document.getElementById('HDDTitle' + i);
                Plus_Minus = document.getElementById('PinnedIconHDD' + i);
            }

            if (document.getElementById(Section + 'PropsRowsStartHere' + i) == null) {
                RowHeader.style.backgroundColor = 'rgb(83,84,85)';
                RowHeader.style.color = 'rgb(249,249,249)';

                Plus_Minus.style.backgroundColor = 'transparent';
                Plus_Minus.style.color = 'rgb(249,249,249)';
            } else {
                var ExtraContentForDAndSP = document.getElementById(Section + 'PropsRowsStartHere' + i);

                RowHeader.style.backgroundColor = 'rgb(83,84,85)';
                RowHeader.style.color = 'rgb(249,249,249)';

                Plus_Minus.style.backgroundColor = 'transparent';
                Plus_Minus.style.color = 'rgb(249,249,249)';
            }
        }

        function SetRowHeaderforContentMouseOut(i, Section) {
            var DAndSPWholeRow = document.getElementById(Section + 'WholeRow' + i);
            var RowHeader = "";
            var Plus_Minus = "";

            if (Section == "DAndSP") {
                RowHeader = document.getElementById('DriversAndSoftpaqsTitle' + i);
                Plus_Minus = document.getElementById('PinnedIconDAndSP' + i);
            } else if (Section == "HDD") {
                RowHeader = document.getElementById('HDDTitle' + i);
                Plus_Minus = document.getElementById('PinnedIconHDD' + i);
            }
            
            if (Section == "DAndSP") {
                if (DAndSP.DAndSPRecords[i].Pinned == 'Yes') {
                    RowHeader.style.backgroundColor = 'white';
                    RowHeader.style.color = 'rgb(83,84,85)';

                    Plus_Minus.style.color = 'rgb(83,84,85)';
                } else {
                    if (document.getElementById(Section + 'PropsRowsStartHere' + i) != null)
                        var ExtraContentForDAndSP = document.getElementById(Section + 'PropsRowsStartHere' + i);

                    RowHeader.style.backgroundColor = 'white';
                    RowHeader.style.color = 'rgb(83,84,85)';

                    Plus_Minus.style.color = 'rgb(83,84,85)';
                }
            } else if (Section == "HDD") {
                if (HDD.HDDRecords[i].Pinned == 'Yes') {
                    RowHeader.style.backgroundColor = 'white';
                    RowHeader.style.color = 'rgb(83,84,85)';

                    Plus_Minus.style.color = 'rgb(83,84,85)';
                } else {
                    if (document.getElementById(Section + 'PropsRowsStartHere' + i) != null)
                        var ExtraContentForDAndSP = document.getElementById(Section + 'PropsRowsStartHere' + i);

                    RowHeader.style.backgroundColor = 'white';
                    RowHeader.style.color = 'rgb(83,84,85)';

                    Plus_Minus.style.color = 'rgb(83,84,85)';
                }
            }
        }

        function ExpandAllRecs(Section) {
            var i = 0;

            if (Section == "DAndSP") {
                if (DAndSP.DAndSPRecords.length > 0) {
                    while (DAndSP.DAndSPRecords[i]) {
                        var Pinned = document.getElementById('PinnedIcon' + Section + i);
                        var PlusMinusElement = document.getElementById(Section + 'PM' + i);

                        if (DAndSP.Expanded == 'No') {
                            document.getElementById(Section + 'PropsRowsStartHere' + i).style.height = document.getElementById(Section + 'PropsRowsStartHere' + i).getAttribute('rowheight');
                            DAndSP.DAndSPRecords[i].Pinned = 'Yes';
                            //setTimeout(function () { PlusMinusElement.innerHTML = '-'; }, 0900);
                            PlusMinusElement.innerHTML = '-';
                        }
                        else {
                            document.getElementById(Section + 'PropsRowsStartHere' + i).removeAttribute('style');
                            DAndSP.DAndSPRecords[i].Pinned = 'No';
                            //setTimeout(function () { PlusMinusElement.innerHTML = '+'; }, 0900);
                            PlusMinusElement.innerHTML = '+';
                        }

                        i++;
                    }

                    SetHeightAttr(Section + 'BodyStartsHere', DAndSP.Expanded, Section);
                    DAndSP.Expanded = DAndSP.Expanded == 'No' ? 'Yes' : 'No';
                }
            } else if (Section == "HDD") {
                if (HDD.HDDRecords.length > 0) {
                    while (HDD.HDDRecords[i]) {
                        var Pinned = document.getElementById('PinnedIcon' + Section + i);
                        var PlusMinusElement = document.getElementById(Section + 'PM' + i);

                        if (HDD.Expanded == 'No') {
                            document.getElementById(Section + 'PropsRowsStartHere' + i).style.height = document.getElementById(Section + 'PropsRowsStartHere' + i).getAttribute('rowheight');
                            HDD.HDDRecords[i].Pinned = 'Yes';
                            document.getElementById('HDDPM' + i).innerHTML = '-';
                        }
                        else {
                            document.getElementById(Section + 'PropsRowsStartHere' + i).removeAttribute('style');
                            HDD.HDDRecords[i].Pinned = 'No';
                            document.getElementById('HDDPM' + i).innerHTML = '+';
                        }

                        i++;
                    }

                    SetHeightAttr(Section + 'BodyStartsHere', HDD.Expanded, Section);
                    HDD.Expanded = HDD.Expanded == 'No' ? 'Yes' : 'No';
                }
            }
        }

        function SetHeightAttr(DivName, Exp_Coll, Section) {
            if (Exp_Coll == 'Yes') {
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;'; }, 0900);
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapsePic').src = 'data:image/png;base64,'; }, 0900);
            } else {
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapse').innerHTML = 'Hide&nbsp;&nbsp;'; }, 0900);
                setTimeout(function () { document.getElementById(Section + 'ExpandCollapsePic').src = 'data:image/png;base64,'; }, 0900);
            }
        }

        function SortByName(x, y) {
            return ((x.Name == y.Name) ? 0 : ((x.Name > y.Name) ? 1 : -1));
        }

        function SetRowHeaderforContentHDDMouseOver(i) {
            var RowHeader = document.getElementById('HDDTitle' + i);

            RowHeader.style.backgroundColor = 'rgb(83,84,85)';
            RowHeader.style.color = 'rgb(249,249,249)';

            if (document.getElementById('HDDPropsRowsStartHere' + i) != null) {
                var ExtraContentForHDD = document.getElementById('HDDPropsRowsStartHere' + i);

                ExtraContentForHDD.removeAttribute('style');
                ExtraContentForHDD.style.backgroundColor = 'rgb(244, 244, 242)';
            }
        }

        function SetRowHeaderforContentHDDMouseOut(i) {
            var DAndSPWholeRow = document.getElementById('HDDWholeRow' + i);
            var RowHeader = document.getElementById('HDDTitle' + i);
            if (document.getElementById('HDDPropsRowsStartHere' + i) != null) {
                var ExtraContentForHDD = document.getElementById('HDDPropsRowsStartHere' + i);

                ExtraContentForHDD.style.backgroundColor = 'white';
            }

            RowHeader.style.backgroundColor = 'white';
            RowHeader.style.color = 'rgb(83,84,85)';
            if (document.getElementById('HDDPropsRowsStartHere' + i) != null) ExtraContentForHDD.removeAttribute('style');
            DAndSPWholeRow.style.WebkitTransitionDelay = '0.5s';
            DAndSPWholeRow.style.transitionDelay = '0.5s';
            DAndSPWholeRow.removeAttribute('style');
        }

        function PrintableView(show_it) {
            var PrintVar = document.getElementById('PrintVer');

            PrintVar.style.Webkittransition = 'opacity 0.45s';
            PrintVar.style.transition = 'opacity 0.45s';

            if (show_it) {
                PrintVar.style.opacity = '1';
            } else {
                PrintVar.style.opacity = '0';
            }
        }
    </script>

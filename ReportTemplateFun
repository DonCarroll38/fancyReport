    <script type='text/javascript'>
        //window.onresize = CalculateHeightOnEachRow();

        var prntVersion = false;
        var hoverEffect = true;
        var hoverEvents;

        var getURLQueryString = function () {
            var url = window.location.href;

            var PrintVer = url.split('?');

            if (PrintVer[1] != undefined) {
                var PrintVarValue = PrintVer[1].split('=');

                prntVersion = PrintVarValue[1] === 'Yes' ? true : false;
                hoverEffect = prntVersion == true ? false : true;
            }
        }

        function addZero(i) {
            if (i < 10) {
                i = '0' + i;
            }
            return i;
        }

        function LoadPage() {
            //Report DateTime
            var d = new Date();
            var ampm = '';
            var hours = 0;

            if (d.getHours() > 12) {
                hours = d.getHours() - 12;
                ampm = 'pm';
            } else {
                hours = d.getHours();
                ampm = 'am';
            }

            getURLQueryString();

            document.getElementById('ReportDateTime').innerHTML = d.getDay() + '/' + d.getMonth() + '/' + d.getFullYear() + '&nbsp;&nbsp;' + addZero(hours) + ':' + addZero(d.getMinutes()) + ':' + addZero(d.getSeconds()) + ' ' + ampm;

            LoadData.HardwareCategory_Load();
            LoadData.OperatingSystemCategory_Load();

            if (prntVersion === false) {
                setTimeout(function () { ReportSectionsFadeIn('BasicInfoCard') }, 0010);

                LoadData.BIOSettings_Load();
                setTimeout(function () { ReportSectionsFadeIn('BIOSSCard') }, 0500);

                LoadData.WindowsUpdate_Load();
                setTimeout(function () { ReportSectionsFadeIn('WUCard') }, 0800);

                LoadData.DAndSP_Load();
                setTimeout(function () { ReportSectionsFadeIn('DAndSPCard') }, 1100);

                LoadData.HDD_Load();
                setTimeout(function () { ReportSectionsFadeIn('HDDCard') }, 1400);

                LoadData.Eng_Load();
                setTimeout(function () { ReportSectionsFadeIn('EngCard') }, 1700);

                LoadData.CrashDump_Load();
                setTimeout(function () { ReportSectionsFadeIn('CDCard') }, 2000);

                var url = window.location.href;

                document.getElementById('PrintLink').setAttribute('href', url + '?PrintVer=Yes');
            } else {
                document.getElementById('BasicInfoCard').classList.remove('CardColor');
                document.getElementById('BasicInfoCard').classList.add('CardColorStatic');
                document.getElementById('BasicInfoCard').style.opacity = '1';

                LoadData.BIOSettings_Load();
                document.getElementById('BIOSSCardBottom').classList.remove('CardColor');
                document.getElementById('BIOSSCardBottom').style.opacity = '1';
                document.getElementById('BIOSSCardBottom').classList.add('CardColorStatic');

                LoadData.WindowsUpdate_Load();
                document.getElementById('WUCard').classList.remove('CardColor');
                document.getElementById('WUCard').classList.add('CardColorStatic');
                document.getElementById('WUCard').style.opacity = '1';
                document.getElementById('WinUpdBodyGoesHere').style.height = 'auto';

                LoadData.DAndSP_Load();
                document.getElementById('DAndSPCard').classList.remove('CardColor');
                document.getElementById('DAndSPCard').classList.add('CardColorStatic');
                document.getElementById('DAndSPCard').style.opacity = '1';

                LoadData.HDD_Load();
                document.getElementById('HDDCard').classList.remove('CardColor');
                document.getElementById('HDDCard').classList.add('CardColorStatic');
                document.getElementById('HDDCard').style.opacity = '1';

                LoadData.Eng_Load();
                document.getElementById('EngCard').classList.remove('CardColor');
                document.getElementById('EngCard').classList.add('CardColorStatic');
                document.getElementById('EngCard').style.opacity = '1';
                document.getElementById('EngBodyGoesHere').style.height = 'auto';

                LoadData.CrashDump_Load();
                document.getElementById('CDCard').classList.remove('CardColor');
                document.getElementById('CDCard').classList.add('CardColorStatic');
                document.getElementById('CDCard').style.opacity = '1';
                document.getElementById('CDBodyGoesHere').style.height = 'auto';

                document.getElementById('PrintLink').style.display = 'none';
            }
        }

        var LoadData = {
            HardwareCategory_Load: function () { LoadHardware(); },
            OperatingSystemCategory_Load: function () { LoadOSInfo(); },
            BIOSettings_Load: function () { LoadBIOSSettings(); },
            WindowsUpdate_Load: function () { LoadWU(); },
            DAndSP_Load: function () { LoadDAndSP(); },
            HDD_Load: function () { LoadHDD(); },
            Eng_Load: function () { LoadEng(); },
            CrashDump_Load: function () { LoadCrashDumps(); }
        }

        function LoadHardware() {
            document.getElementById('ProductRep').innerHTML = Hardware.ProductRep;
            document.getElementById('SerialNoRep').innerHTML = Hardware.SerialNo;
            document.getElementById('SKURep').innerHTML = Hardware.SKU;
            document.getElementById('BIOSVerRep').innerHTML = Hardware.BIOSVers;
            document.getElementById('UUID').innerHTML = Hardware.UUID;
            document.getElementById('ProcessorType').innerHTML = Hardware.ProcessorType;
            document.getElementById('BaseBoardProductId').innerHTML = Hardware.BaseBoardProductId;
            document.getElementById('BaseBoardSerialNumber').innerHTML = Hardware.BaseBoardSerialNumber;
            document.getElementById('BaseBoardVersion').innerHTML = Hardware.BaseBoardVersion;
        }

        function LoadOSInfo() {
            document.getElementById('OSRep').innerHTML = OperatingSys.OS;
            document.getElementById('LanguageDisplayNameRep').innerHTML = OperatingSys.LanguageDisplayName;
            document.getElementById('ArchitectureRep').innerHTML = OperatingSys.Architecture;
            if (OperatingSys.LanguageDisplayName != OperatingSys.LanguageNativeName) {
                document.getElementById('LanguageNativeName').innerHTML = OperatingSys.LanguageNativeName;
            }
            else {
                document.getElementById('LangNativeName').style.display = 'none';
            }
        }

        function LoadBIOSSettings() {
            var BIOSDiv;

            if (prntVersion === true) {
                BIOSDiv = 'Bottom';
                document.getElementById('BIOSSCard').style.display = 'none';
                document.getElementById('BIOSSCardBottom').style.display = 'inline-block';
                document.getElementById('BIOSCardSpace').style.display = 'none';
            } else {
                BIOSDiv = '';
                document.getElementById('BIOSSCard').style.display = 'inline-block';
                document.getElementById('BIOSSCardBottom').style.display = 'none';
            }

            document.getElementById('BIOSIssueText' + BIOSDiv).innerHTML = '&nbsp;' + BIOSSettingsData.Message;
            var i = 0;
            var j = 0;
            var RowCnt = 0;
            var strRow = '';
            var strRowsCollection = '';

            if (BIOSSettingsData.BIOSSettingsRecords.length > 0) {
                strRowsCollection = '<div id="BIOS_RowHeaders" class="RowShellHeader row">'
                + '<div class="w3-col s4 m4 l4" style="text-align:right;padding-right:20px;">Name</div>'
                + '<div class="w3-col s8 m8 l8" style="text-align:left;padding-left:20px">Possible Values</div>'
                + '</div>'
                + '<div class="w3-row BtmBorder"></div>'
                + '<div class="w3-row topBorder"></div>';

                while (BIOSSettingsData.BIOSSettingsRecords[i]) {
                    if (BIOSSettingsData.BIOSSettingsRecords[i].Name != ' ' && BIOSSettingsData.BIOSSettingsRecords[i].Name != '') {
                        strRow = '<div id="BIOS_RF' + RowCnt + '" class="BIOSRowFormat w3-row" style="font-size:small;">'
                                   + '<div id="BIOS_RH' + RowCnt + '" class="RowHeader">'
                                       + '<div class="w3-col s4 m4 l4 CenterText" style="text-align:right;padding-right:20px;vertical-align:center;padding-bottom:5px;">' + BIOSSettingsData.BIOSSettingsRecords[i].Name + ':</div>'
                                       + '<div id="BIOS_RHShown' + RowCnt + '" class="w3-col s8 m8 l8" style="text-align:left;padding-left:20px;vertical-align:center;padding-bottom:5px;">';

                        j = 0;

                        while (BIOSSettingsData.BIOSSettingsRecords[i].Possiblities[j]) {
                            strRow += (j == 0) ? '' : '  |  '
                            if (BIOSSettingsData.BIOSSettingsRecords[i].Possiblities[j].Option == BIOSSettingsData.BIOSSettingsRecords[i].Value) strRow += '<span class="SelectedValue">&nbsp;&nbsp;';

                            strRow += BIOSSettingsData.BIOSSettingsRecords[i].Possiblities[j].Option;

                            if (BIOSSettingsData.BIOSSettingsRecords[i].Possiblities[j].Option == BIOSSettingsData.BIOSSettingsRecords[i].Value) strRow += '&nbsp;&nbsp;</span>';

                            j++;
                        }

                        strRow += '</div>'
                               + '</div>'
                               + '</div>'
                               // + '<div class="w3-row BtmBorder"></div>' // bottom border of rows
                               + '<div class="w3-row topBorder"></div>';

                        strRowsCollection += strRow;

                        RowCnt++;
                    }

                    i++;
                }

                document.getElementById('BIOSBodyGoesHere' + BIOSDiv).innerHTML = strRowsCollection;

                strRow = '';
                strRow += '<table id="BIOSFooter"><tr><th id="BIOSFooterth" colspan="3">'
                strRow += '<div style="height:10px;"></div>'
                strRow += '<span class="SelectedValue">&nbsp;' + RowCnt + ' </span>'
                strRow += '&nbsp;BIOS Settings</th></tr></table>'
                document.getElementById('BIOSBodyGoesHere' + BIOSDiv).innerHTML = document.getElementById('BIOSBodyGoesHere' + BIOSDiv).innerHTML + strRow;

                BIOSSettingsData.Message = '<span class="SelectedValue">&nbsp;' + RowCnt + ' </span>&nbsp;BIOS Settings';
                document.getElementById('BIOSIssueText' + BIOSDiv).innerHTML = '&nbsp;' + BIOSSettingsData.Message;

                if (prntVersion === false) {
                    CalculateHeightOnEachBIOSRow();

                    SectionHeader(BIOSSettingsData.BIOSSettingsRecords.length, 'BIOS');
                } else {
                    SectionHeader(0, 'BIOSBottom');
                }
            } else {
                SectionHeader(0, 'BIOS');
            }
        }

        function LoadWU() {
            //Windows Updates
            document.getElementById('WUIssueText').innerHTML = '&nbsp;' + WinUpdates.WinUpdatesMessage;
            var strHeaderSection = '';
            var i = 0;
            var strRow = '';

            if (WinUpdates.WURecords.length > 0) {
                strHeaderSection = '<div id="WU_RowHeaders" class="RowShellHeader row">'
                + '<div class="w3-col s2 m2 l2">KB Article</div>'
                + '<div class="w3-col s8 m8 l8" style="text-align:left;">Title</div>'
                + '<div class="w3-col s2 m2 l2">Security Bulletin</div>'
                + '</div>'
                + '<div class="w3-row BtmBorder"></div>'
                + '<div class="w3-row topBorder"></div>';

                document.getElementById('WinUpdBodyGoesHere').innerHTML = strHeaderSection;

                var KBArticleIds = '';
                var Title = '';
                var SecurityBulletinIDs = '';
                var Description = '';

                while (WinUpdates.WURecords[i]) {
                    KBArticleIds = WinUpdates.WURecords[i].KBArticleIDs == 'Null' ? '&nbsp;' : WinUpdates.WURecords[i].KBArticleIDs;
                    Title = WinUpdates.WURecords[i].Title == 'Null' ? '&nbsp;' : WinUpdates.WURecords[i].Title;
                    SecurityBulletinIDs = WinUpdates.WURecords[i].SecurityBulletinIDs == 'Null' ? '&nbsp;' : '<a id="SBLink' + i + '" href=https://support.microsoft.com/en-us/kb/' + KBArticleIds + ' target="_blank">' + WinUpdates.WURecords[i].SecurityBulletinIDs + '</a>'
                    Description = WinUpdates.WURecords[i].Description == 'Null' ? '&nbsp;' : WinUpdates.WURecords[i].Description;
                    
                    hoverEvents = hoverEffect === true ? ' onmouseover="SetRowHeaderforContentWUMouseOver(' + i + ');" onmouseout="SetRowHeaderforContentWUMouseOut(' + i + ');"' : '';

                    strRow = '<div id="WU_RF' + i + '" class="RowFormat w3-row" ' + hoverEvents + '>'
                               + '<div id="WU_RH' + i + '" class="RowHeader">'
                                   + '<div class="w3-col s2 m2 l2 CenterText" style="padding-bottom:5px;"><a id="KBAIDLink' + i + '" class="HighlightedRowLink" href=https://support.microsoft.com/en-us/kb/' + KBArticleIds + ' target="_blank">' + KBArticleIds + '</a></div>'
                                   + '<div id="WU_RHShown' + i + '" class="w3-col s8 m8 l8">' + Title + '</div>'
                                   + '<div class="w3-col s2 m2 l2 CenterText">' + SecurityBulletinIDs + '</div>'
                               + '</div>'
                               + '<div id="WU_EC' + i + '" class="w3-row"><div class="w3-col s12 m12 l12">' + Description + '</div></div>'
                           + '</div>'
                           // + '<div class="w3-row BtmBorder"></div>' // bottom border of rows
                           + '<div class="w3-row topBorder"></div>'

                    document.getElementById('WinUpdBodyGoesHere').innerHTML = document.getElementById('WinUpdBodyGoesHere').innerHTML + strRow;

                    i++;
                }

                strRow = '';
                strRow += '<table id="WUFooter"><tr><th id="WUFooterth" colspan="3">'
                strRow += '<div style="height:10px;"></div>'
                strRow += '<span class="SelectedValue">&nbsp;' + WinUpdates.WURecords.length + ' </span>'
                strRow += '&nbsp;Uninstalled Windows Updates found</th></tr></table>'
                document.getElementById('WinUpdBodyGoesHere').innerHTML = document.getElementById('WinUpdBodyGoesHere').innerHTML + strRow;

                //WinUpdates.WinUpdatesMessage = RowCnt + '&nbsp;BIOS Settings';
                document.getElementById('WUIssueText').innerHTML = document.getElementById('WUIssueText').innerHTML + '<br />&nbsp;<span class="SelectedValue">&nbsp;' + WinUpdates.WURecords.length + ' </span>&nbsp;Uninstalled Windows Updates found';

                if (prntVersion === false) {
                    CalculateHeightOnEachWURow();

                    SectionHeader(WinUpdates.WURecords.length, "WU");
                } else {
                    SectionHeader(0, "WU");
                }
            } else {
                SectionHeader(0, "WU");
            }
        }

        function LoadDAndSP() {
            var strHeaderSection = '';
            var i = 0;
            var j = 0;
            var strRow = '';

            //Drivers and Softpaqs
            if (DAndSP.DAndSPRecords.length > 0) {
                strRow = '';
                j = 0;

                document.getElementById('DAndSPIssueText').innerHTML = '&nbsp;' + DAndSP.Message;
        
                while (DAndSP.DAndSPRecords[i]) {
                    hoverEvents = hoverEffect === true ? ' onmouseover="SetRowHeaderforContentMouseOver(' + i + ', &apos;DAndSP&apos;);" onmouseout="SetRowHeaderforContentMouseOut(' + i + ', &apos;DAndSP&apos;);"' : '';

                    strRow += '<div id="DAndSPWholeRow' + i + '" class="w3-row RowFormat"' + hoverEvents + '>'
                    strRow += '<div class="w3-col s12 m12 l12">';
                    strRow += '<div id="DriversAndSoftpaqsTitle' + i + '" class="w3-row RowHeader" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;"><a class="Plus_Minus_Sign" href="#"><div id="PinnedIconDAndSP' + i + '" class="Plus_Minus_Sign DrivAndSofPqsHeaderTitle RowHeader" style="float:left;width:3%;"><div id="DAndSPPM' + i + '" style="position:relative;top:-5px;">+</div></div></a>';
                    strRow += '<div style="float:left;width:97%;" class="DrivAndSofPqsHeaderTitle">' + DAndSP.DAndSPRecords[i].Title + '</div>';
                    strRow += '</div>';

                    //Properties go here.*********************************************************************************************************************************************************
                    strRow += '<div class="w3-row" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;"><div class="w3-col s12 m12 l12" style="height:5px;">&nbsp;</div></div>'
                    strRow += '<div class="w3-row">';
                    strRow += '<div id="DAndSPPropsRowsStartHere' + i + '" class="w3-col s12 m12 l12">'

                    if (DAndSP.DAndSPRecords[i].Properties.length > 0) {

                        j = 0;
                        while (DAndSP.DAndSPRecords[i].Properties[j]) {
                            strRow += '<div class="w3-row">'
                            strRow += '<div class="w3-col s5 m4 l3" style="text-align:right;padding-right:5px;" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;">' + DAndSP.DAndSPRecords[i].Properties[j].PropName + ':&nbsp;</div>';

                            if (DAndSP.DAndSPRecords[i].Properties[j].Hyperlink == 'False' || DAndSP.DAndSPRecords[i].Properties[j].Hyperlink == 'false') {
                                strRow += '<div class="w3-col s7 m8 l9" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;" style="text-align:left;padding-left:5px;">' + DAndSP.DAndSPRecords[i].Properties[j].Value + '</div>';
                            }
                            else { //This is here for when this row is a hyperlink.
                                strRow += '<div class="w3-col s7 m8 l9" style="text-align:left;padding-left:5px;"><a href="' + DAndSP.DAndSPRecords[i].Properties[j].Value + '" target="_blank">' + DAndSP.DAndSPRecords[i].Properties[j].Value + '</a></div>';
                            }

                            strRow += '</div>'
                            j++;
                        }

                    } else {
                        strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                        strRow += '<div class="w3-row">'
                        strRow += '<div class="w3-col s12 m12 l12" style="text-align:center;padding-right:5px;">No information is available...</div>';
                        strRow += '</div>'
                        strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                    }

                    strRow += '<div class="w3-row" style="height:5px;" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;">&nbsp;</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    //****************************************************************************************************************************************************************************

                    strRow += '<div class="w3-row">'
                    strRow += '<div id="RowTableRaisedBoxDAndSP' + i + '" class="w3-col s12 m12 l12 RowTableRaisedBox">'
                    strRow += '<div class="w3-row RowShellHeaderforDriversAndSoftPaqs" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;">'
                    strRow += '<div class="w3-col s12 m12 l12" style="text-align:left">'
                    strRow += '&nbsp;&nbsp;Issue'
                    strRow += '</div>'
                    strRow += '</div>'

                    if (DAndSP.DAndSPRecords[i].Issues.length > 0) {
                        j = 0;
                        while (DAndSP.DAndSPRecords[i].Issues[j]) {
                            strRow += '<div class="w3-row">'
                            strRow += '<div class="w3-col s12 m12 l12" style="text-align:left">&nbsp;&nbsp;' + DAndSP.DAndSPRecords[i].Issues[j].Issue + '</div>'
                            strRow += '</div>'
                            j++;
                        }
                    }
                    else {
                        strRow += '<div class="w3-row" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;">'
                        strRow += '<div class="w3-col s12 m12 l12" style="text-align:left">&nbsp;&nbsp;No Issue Recommendations found</div>'
                        strRow += '</div>'
                    }

                    strRow += '<div class="w3-row" style="height:5px;" onclick="Pin_UnPin(' + i + ', &apos;DAndSP&apos;); return false;">&nbsp;</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    strRow += '<div class="w3-row" style="height:5px;"><div class="w3-col s12 m12 l12" style="height:15px;">&nbsp;</div></div>'

                    i++;
                }

                strRow += '<table>'
                strRow += '<tr>'
                strRow += '<th colspan="3">'
                strRow += '<div style="height:10px;"></div>'
                strRow += '&nbsp;&nbsp;<span class="SelectedValue">&nbsp;' + i + ' </span>'
                strRow += '&nbsp;Drivers and Softpaqs problems were found'
                strRow += '</th>'
                strRow += '</tr>'
                strRow += '</table>'

                document.getElementById('DAndSPBodyStartsHere').innerHTML = strRow;

                if (prntVersion === false) {
                    CalculateHeightOnEachExpandRow('DAndSP');

                    SectionHeader(DAndSP.DAndSPRecords.length, "DAndSP");
                } else {
                    SectionHeader(0, "DAndSP");
                }
            } else {
                SectionHeader(0, "DAndSP");

                document.getElementById('DAndSPIssueText').innerHTML = '&nbsp;' + DAndSP.Message;
            }
        }

        function LoadHDD() {
            var strHeaderSection = '';
            var i = 0;
            var j = 0;
            var k = 0;
            var n = 0;
            var y = 0;

            //Drive Information
            if (HDD.HDDRecords.length > 0) {
                strRow = '';
                j = 0;

                document.getElementById('HDDIssueText').innerHTML = '&nbsp;' + HDD.Message;
                
                //<div style='font-weight:bolder;color:rgb(83, 84, 85);font-size:x-large;'>Drive Information</div>

                while (HDD.HDDRecords[i]) {
                    hoverEvents = hoverEffect === true ? ' onclick="Pin_UnPin(' + i + ',&apos;HDD&apos;); return false;"  onmouseover="SetRowHeaderforContentMouseOver(' + i + ', &apos;HDD&apos;);" onmouseout="SetRowHeaderforContentMouseOut(' + i + ', &apos;HDD&apos;);"' : '';

                    strRow += '<div id="HDDWholeRow' + i + '" class="w3-row RowFormat"' + hoverEvents + '>'
                    strRow += '<div class="w3-col s12 m12 l12">';
                    strRow += '<div id="HDDTitle' + i + '" class="w3-row RowHeader"><a class="Plus_Minus_Sign" href="#"><div id="PinnedIconHDD' + i + '" style="float:left;width:3%;" class="Plus_Minus_Sign DrivAndSofPqsHeaderTitle RowHeader"><div id="HDDPM' + i + '" style="position:relative;top:-5px;">+</div></div></a>';
                    strRow += '<div style="float:left;width:97%;" class="DrivAndSofPqsHeaderTitle">&nbsp;&nbsp;' + HDD.HDDRecords[i].Title + '</div>';
                    strRow += '</div>';

                    //if (HDD.HDDRecords[i].HDDProperties.length > 0) {
                    strRow += '<div class="w3-row"><div class="w3-col s12 m12 l12" style="height:5px;">&nbsp;</div></div>'
                    strRow += '<div id="HDDPropsRowsStartHere' + i + '" class="w3-row">';
                    strRow += '<div class="w3-col s12 m12 l6">'

                    j = 0;
                    while (HDD.HDDRecords[i].HDDProperties[j]) {
                        strRow += '<div class="w3-row">'
                        strRow += '<div class="w3-col s6 m6 l6" style="text-align:right;padding-right:5px;">' + HDD.HDDRecords[i].HDDProperties[j].HDDName + ':&nbsp;</div>';

                        //if (HDD.HDDRecords[i].Properties[j].Hyperlink == 'false') {
                        strRow += '<div class="w3-col s6 m6 l6" style="text-align:left;padding-left:5px;">' + HDD.HDDRecords[i].HDDProperties[j].HDDValue + '</div>';
                        /*}
                        else { //This is here for when this row is a hyperlink.
                            strRow += '<div class="w3-col s7 m8 l9" style="text-align:left;padding-left:5px;"><a href="' + HDD.HDDRecords[i].HDDProperties[j].HDDName + '" target="_blank">' + HDD.HDDRecords[i].HDDProperties[j].HDDValue + '</a></div>';
                        }*/

                        strRow += '</div>'


                        j++;
                    }

                    strRow += '</div>'
                    strRow += '<div class="w3-col s12 m12 l6">'

                    j = 0;
                    if (HDD.HDDRecords[i].Partitions[j]) {
                        strRow += '<div class="w3-row"><div class="w3-col s12 m12 l12" style="height:5px;">&nbsp;</div></div>'
                        strRow += '<div class="w3-row">';
                        strRow += '<div id="HDDPartitionRowsStartHere' + i + '" class="w3-col s12 m12 l12 PartitionBox" style="margin-left:5px;margin-right:5px;width:95%;text-align:center;">'
                        strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'

                        j = 0;
                        while (HDD.HDDRecords[i].Partitions[j]) {
                            strRow += '<div class="w3-row">'
                            strRow += '<div class="w3-col s12 m12 l12" style="text-align:left;padding-right:5px;"><span style="font-weight:bold;">&nbsp;&nbsp;&nbsp;&nbsp;' + HDD.HDDRecords[i].Partitions[j].PartName + '</span>';
                            strRow += '<div class="w3-row">'
                            strRow += '<div class="w3-col s12 m6 l6" style="padding-left:5px;padding-right:5px;">'
                            k = 0;
                            if (HDD.HDDRecords[i].Partitions[j].PartProps[k]) {
                                while (HDD.HDDRecords[i].Partitions[j].PartProps[k]) {
                                    strRow += '<div class="w3-row">'
                                    strRow += '<div class="w3-col s6 m6 l6" style="text-align:right;padding-right:5px;">&nbsp;&nbsp;&nbsp;&nbsp;' + HDD.HDDRecords[i].Partitions[j].PartProps[k].PartName + ':&nbsp;</div>';
                                    strRow += '<div class="w3-col s6 m6 l6" style="text-align:left;padding-left:5px;">&nbsp;&nbsp;&nbsp;&nbsp;' + HDD.HDDRecords[i].Partitions[j].PartProps[k].PartValue + '</div>';
                                    strRow += '</div>'

                                    k++;
                                }
                            }

                            strRow += '</div>'
                            strRow += '<div class="w3-col s12 m6 l6" style="padding-left:5px;padding-right:5px;">'
                            if (HDD.HDDRecords[i].Partitions[j].LocalDisks.length > 0)
                                strRow += '<div class="w3-row LocalDiskBox">'

                            y = 0;
                            if (HDD.HDDRecords[i].Partitions[j].LocalDisks[y]) {
                                //strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                                //json.sort(HDD.HDDRecords, "ascending", "Title");
                                while (HDD.HDDRecords[i].Partitions[j].LocalDisks[y]) {
                                    strRow += '<div class="w3-row">'
                                    //strRow += '<div class="w3-col s1 m1 l1">&nbsp;</div>'
                                    if (HDD.HDDRecords[i].Partitions[j].LocalDisks.length > 1) {
                                        if (y == 0) { //Top Row - border radius on the top corners.
                                            strRow += '<div class="w3-col s12 m12 l12 LocalDiskBoxTop" style="text-align:left;padding-right:5px;">'
                                            strRow += '<span style="font-weight:bold;">'
                                            strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                                        }
                                        else if (y < (HDD.HDDRecords[i].Partitions[j].LocalDisks.length - 1)) {//Middle row - no border radius on any of the 4 corners.  This will happen only when there are at least 3 Local Disks rows.
                                            strRow += '<div class="w3-col s12 m12 l12 LocalDiskBoxTop" style="text-align:left;padding-right:5px;">'
                                            strRow += '<span style="font-weight:bold;">'
                                        }
                                        else { //Botom Row - border radius on the bottom corners.
                                            strRow += '<div class="w3-col s12 m12 l12 LocalDiskBoxBottom" style="text-align:left;padding-right:5px;">'
                                            strRow += '<span style="font-weight:bold;">'
                                        }
                                    } else {
                                        strRow += '<div class="w3-col s12 m12 l12 LocalDiskBoxWhole" style="text-align:left;padding-right:5px;">'
                                        strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                                        strRow += '<span style="font-weight:bold;">'
                                    }
                                    strRow += '&nbsp;&nbsp;&nbsp;&nbsp;' + HDD.HDDRecords[i].Partitions[j].LocalDisks[y].LocalDisksName + ' Drive - Local Fixed Disk</span>';

                                    n = 0;
                                    if (HDD.HDDRecords[i].Partitions[j].LocalDisks[y].LocalProps[n]) {
                                        while (HDD.HDDRecords[i].Partitions[j].LocalDisks[y].LocalProps[n]) {
                                            strRow += '<div class="w3-row">'
                                            strRow += '<div class="w3-col s6 m6 l6" style="text-align:right;padding-right:5px;">' + HDD.HDDRecords[i].Partitions[j].LocalDisks[y].LocalProps[n].LocalName + ':&nbsp;</div>';
                                            strRow += '<div class="w3-col s6 m6 l6" style="text-align:left;padding-left:5px;">' + HDD.HDDRecords[i].Partitions[j].LocalDisks[y].LocalProps[n].LocalValue + '</div>';
                                            strRow += '</div>'

                                            n++;
                                        }
                                    }

                                    if (HDD.HDDRecords[i].Partitions[j].LocalDisks.length > 1) {
                                        if (y == 0) {
                                            strRow += '<div class="w3-row" style="margin-left:20px;margin-right:20px;height:20px;border-bottom:1px solid rgb(128,189,87);">&nbsp;</div>'
                                            strRow += '<div class="w3-row" style="margin-left:20px;margin-right:20px;height:15px;">&nbsp;</div>'
                                        }
                                        else if (y < (HDD.HDDRecords[i].Partitions[j].LocalDisks.length - 1)) {
                                            strRow += '<div class="w3-row" style="margin-left:20px;margin-right:20px;height:20px;border-bottom:1px solid rgb(128,189,87);">&nbsp;</div>'
                                            strRow += '<div class="w3-row" style="margin-left:20px;margin-right:20px;height:15px;">&nbsp;</div>'
                                        } else {
                                            strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                                        }
                                    } else {
                                        strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                                    }
                                    strRow += '</div>'
                                    strRow += '</div>'

                                    y++;
                                }
                            }

                            if (HDD.HDDRecords[i].Partitions[j].LocalDisks.length > 0)
                                strRow += '</div>'

                            strRow += '</div>'
                            strRow += '</div>'

                            if (HDD.HDDRecords[i].Partitions.length > 1) {
                                if (j == 0 || j < (HDD.HDDRecords[i].Partitions.length - 1)) {
                                    strRow += '<div class="w3-row" style="margin-left:20px;margin-right:20px;height:20px;border-bottom:1px solid rgb(162,198,253);">&nbsp;</div>'
                                    strRow += '<div class="w3-row" style="margin-left:20px;margin-right:20px;height:15px;">&nbsp;</div>'
                                } else {
                                    strRow += '<div class="w3-row" style="height:15px;">&nbsp;</div>'
                                }
                            } else {
                                strRow += '<div class=""w3-row"" style=""margin-left:20px;margin-right:20px;height:20px;border:0;"">&nbsp;</div>'
                            }

                            strRow += '</div>'
                            strRow += '</div>'

                            j++;
                        }

                        strRow += '</div>'
                        strRow += '<div class="w3-col s1 m1 l1">&nbsp;</div>'
                        strRow += '</div>'
                    } else {
                        strRow += '<div class="w3-row">'
                        strRow += '<div class="w3-col s12 m12 l12">&nbsp;&nbsp;&nbsp;&nbsp;No Partition Information Available...</div>'
                        strRow += '</div>'
                    }

                    strRow += '<div class="w3-row" style="height:20px;">&nbsp;</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    //****************************************************************************************************************************************************************************

                    strRow += '<div class="w3-row">'
                    strRow += '<div id="RowTableRaisedBoxHDD' + i + '" class="w3-col s12 m12 l12 RowTableRaisedBox">'
                    strRow += '<div class="w3-row RowShellHeaderforDriversAndSoftPaqs">'
                    strRow += '<div class="w3-col s12 m12 l12" style="text-align:left">'
                    strRow += '&nbsp;&nbsp;Issue'
                    strRow += '</div>'
                    strRow += '</div>'

                    if (HDD.HDDRecords[i].Issues.length > 0) {
                        j = 0;
                        while (HDD.HDDRecords[i].Issues[j]) {
                            strRow += '<div class="w3-row">'
                            strRow += '<div class="w3-col s12 m12 l12" style="text-align:left">&nbsp;&nbsp;' + HDD.HDDRecords[i].Issues[j].Issue + '</div>'
                            strRow += '</div>'
                            j++;
                        }
                    }
                    else {
                        strRow += '<div class="w3-row">'
                        strRow += '<div class="w3-col s12 m12 l12" style="text-align:left">&nbsp;&nbsp;No Drive Issue found</div>'
                        strRow += '</div>'
                    }
                    strRow += '<div class="w3-row" style="height:5px;"><div class="w3-col s12 m12 l12" style="height:5px;">&nbsp;</div></div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    strRow += '</div>'
                    strRow += '<div class="w3-row" style="height:5px;"><div class="w3-col s12 m12 l12" style="height:15px;">&nbsp;</div></div>'

                    i++;
                }

                strRow += '<table>'
                strRow += '<tr>'
                strRow += '<th colspan="3">'
                strRow += '<div style="height:10px;"></div>'
                strRow += '&nbsp;&nbsp;<span class="SelectedValue">&nbsp;' + i + ' </span>'
                if (HDD.HDDRecords.length == 1)
                    strRow += '&nbsp;Hard Drive was found'
                else
                    strRow += '&nbsp;Hard Drives were found'
                strRow += '</th>'
                strRow += '</tr>'
                strRow += '</table>'

                document.getElementById('HDDBodyStartsHere').innerHTML = strRow;

                if (prntVersion === false) {
                    CalculateHeightOnEachExpandRow('HDD');

                    SectionHeader(HDD.HDDRecords.length, 'HDD');
                } else {
                    SectionHeader(0, 'HDD');
                }
            } else {
                SectionHeader(0, 'HDD');

                document.getElementById('HDDIssueText').innerHTML = '&nbsp;' + HDD.Message;
            }
        }

        function LoadEng() {
            //Windows Updates
            document.getElementById('EngIssueText').innerHTML = '&nbsp;' + Eng.StatusText;
            strHeaderSection = '';
            i = 0;
            strRow = '';

            if (Eng.EngRecords.length > 0) {
                var EngTitle = '';
                var EngSourceLocation = '';
                var EngCategory = '';
                var b = 0;
                var bolFound = false;

                var Categories = [];
                //Grab all of the Categories and add them to the "Categories" array.
                while (Eng.EngRecords[i]) {
                    EngCategory = Eng.EngRecords[i].Category == 'Null' ? '&nbsp;' : Eng.EngRecords[i].Category;
                    
                    bolFound = false;
                    for (b = 0; b < Categories.length; b++) {
                        if (EngCategory == Categories[b])
                        {
                            bolFound = true;
                        }
                    }
                
                    if (bolFound == false)
                        Categories.push(EngCategory);

                    i++;
                }
                Categories.sort();

                var TitlesTemp = [];
                var Titles = [];
                var item = '';
                i = 0;
                //Grab all of the Titles and add them to the "Titles" array.
                while (Eng.EngRecords[i]) {
                    EngTitle = Eng.EngRecords[i].Title == 'Null' ? '&nbsp;' : Eng.EngRecords[i].SourceLocation;

                    TitlesTemp.push(EngTitle.toLowerCase() + '~' + EngTitle);

                    i++;
                }
                TitlesTemp.sort();
                for (b = 0; b < TitlesTemp.length; b++) {
                    item = TitlesTemp[b].split('~');
                    Titles.push(item[1]);
                }
        
                var m;
                var x;
                var y;
                var s = 0; //This is used to keep track of numeric iterator portion the id for each Eng Row.
                for (m = 0; m < Categories.length; m++) {
                    strRow += '<div class="w3-row" style="height:15px;"></div>'
                    strRow += '<div class="w3-row RowEngShellHeaderHighlighting" style="padding-left:20px;padding-right:20px;padding-bottom:7px;padding-top:1px;">'
                    strRow += '<div class="w3-row"><div class="w3-col s12 m12 l12 RowEngShellHeader DrivAndSofPqsHeaderTitle" style="color:rgb(83,84,85);">&nbsp;&nbsp;' + Categories[m] + '</div></div>'
                    strRow += '<div class="w3-row" style="height:5px;"></div>'
                    strRow += '<div id="Eng_RowHeaders' + m + '" class="w3-row" >'
                    strRow += '<div class="w3-col s10 m10 l10" style="text-align:left;padding-left:10px;font-weight:600;">Source Location</div>'
                    strRow += '<div class="w3-col s2 m2 l2" style="text-align:center;font-weight:600;">FileSize</div>'
                    strRow += '</div>'
                    strRow += '<div class="w3-row BtmBorder"></div>'
                    strRow += '<div class="w3-row topBorder"></div>';

                    //i = 0;
                    //while (Eng.EngRecords[i]) {
                    //    EngCategory = Eng.EngRecords[i].Category == 'Null' ? '&nbsp;' : Eng.EngRecords[i].Category;

                    //    if (EngCategory == Categories[m]) {
                    for (x = 0; x <= Titles.length; x++) {
                        y = 0;
                        bolFound = false;

                        while (Eng.EngRecords[y]) {
                            EngSourceLocation = Eng.EngRecords[y].SourceLocation == 'Null' ? '&nbsp;' : Eng.EngRecords[y].SourceLocation;
                            FileSize = Eng.EngRecords[y].Title == 'Null' ? '&nbsp;' : Eng.EngRecords[y].FileSize;
                            EngCategory = Eng.EngRecords[y].Category == 'Null' ? '&nbsp;' : Eng.EngRecords[y].Category;

                            if (EngCategory == Categories[m] && EngSourceLocation == Titles[x]) {
                                strRow += '<div id="Eng_RH' + s + '" class="w3-row EngRowFormat">'
                                strRow += '<div class="w3-col s10 m10 l10 CenterText" style="padding-bottom:5px;padding-left:10px;text-align:left;">' + EngSourceLocation + '</div>'
                                strRow += '<div class="w3-col s2 m2 l2" style="text-align:center;">' + FileSize + '</div>'
                                strRow += '</div>'

                                s++;
                                break;
                            }

                            y++;
                        }
                    }

                        //    break;
                        //}

                    //    i++;
                    //}

                    strRow += '</div>'
                    document.getElementById('EngBodyGoesHere').innerHTML = document.getElementById('EngBodyGoesHere').innerHTML + strRow;
                    strRow = '';
                }

                strRow = '';
                strRow += '<table id="EngFooter"><tr><th id="EngFooterth" colspan="3">'
                strRow += '<div style="height:10px;"></div>'
                strRow += '<span class="SelectedValue">&nbsp;' + Eng.EngRecords.length + ' </span>'
                strRow += '&nbsp;files found</th></tr></table>'
                document.getElementById('EngBodyGoesHere').innerHTML = document.getElementById('EngBodyGoesHere').innerHTML + strRow;

                if (prntVersion === false) {
                    CalculateHeightOnEachEngRow();

                    SectionHeader(Eng.EngRecords.length, 'Eng');
                } else {
                    SectionHeader(0, 'Eng');
                }
            } else {
                SectionHeader(0, 'Eng');
            }
        }

        function LoadCrashDumps() {
            document.getElementById('CDIssueText').innerHTML = '&nbsp;' + CrashDumps.StatusText;
            var i = 0;
            var j = 0;
            var RowCnt = 0;
            var strRow = '';
            var strRowsCollection = '';

            //document.getElementById('CDBodyGoesHere').innerHTML = "Test!!!";
            if (CrashDumps.CrashDumpRecords.length > 0) {
                strRowsCollection = '<div id="CD_RowHeaders" class="RowShellHeader row">'
                //                + '<div class="w3-col s3 m3 l3 CrashDumpsRow" style="text-align:left;padding-right:20px;">Crash Date/Time</div>'
                + '<div class="w3-col s3 m3 l3 CrashDumpsRow" style="text-align:left;">Crash Date/Time</div>'
                + '<div class="w3-col s3 m3 l3 CrashDumpsRow">Dump Filename</div>'
                + '<div class="w3-col s3 m3 l3 CrashDumpsRow">Error Code</div>'
                + '<div class="w3-col s3 m3 l3 CrashDumpsRow">Probably Caused By</div>'
                + '</div>'
                + '<div class="w3-row BtmBorder"></div>'
                + '<div class="w3-row topBorder"></div>';

                document.getElementById('CDBodyGoesHere').innerHTML = strRowsCollection;

                while (CrashDumps.CrashDumpRecords[i]) {
                    strRow += '<div id="CD_RF' + RowCnt + '" class="w3-row" style="font-size:small;">'
                                + '<div id="CD_RH' + RowCnt + '" class="RowHeader">'
                                    + '<div id="CD_RHShown0' + i + '" class="w3-col s3 m3 l3 CrashDumpsRow">' + CrashDumps.CrashDumpRecords[i].CrashDate + '</div>'
                                    + '<div id="CD_RHShown1' + i + '" class="w3-col s3 m3 l3 CrashDumpsRow">' + CrashDumps.CrashDumpRecords[i].DumpFileName + '</div>'
                                    + '<div id="CD_RHShown2' + i + '" class="w3-col s3 m3 l3 CrashDumpsRow">' + CrashDumps.CrashDumpRecords[i].ErrorCode + '</div>'
                                    + '<div id="CD_RHShown3' + i + '" class="w3-col s3 m3 l3 CrashDumpsRow">' + CrashDumps.CrashDumpRecords[i].ProbCausedBy + '</div>'
                            + '</div>'
                            + '</div>'
                            //+ '<div class="w3-row BtmBorder"></div>'
                            + '<div class="w3-row topBorder"></div>';

                    RowCnt++;
                    i++;
                }

                document.getElementById('CDBodyGoesHere').innerHTML = document.getElementById('CDBodyGoesHere').innerHTML + strRow;

                strRow = '';
                strRow += '<table id="CDFooter"><tr><th id="CDFooterth" colspan="3">'
                strRow += '<div style="height:10px;"></div>'
                strRow += '<span class="SelectedValue">&nbsp;' + RowCnt + ' </span>'
                strRow += '&nbsp;Crash Dumps</th></tr></table>'
                document.getElementById('CDBodyGoesHere').innerHTML = document.getElementById('CDBodyGoesHere').innerHTML + strRow;

                CrashDumps.StatusText = '<span class="SelectedValue">&nbsp;' + RowCnt + ' </span>&nbsp;Crash Dumps';
                document.getElementById('CDIssueText').innerHTML = '&nbsp;' + CrashDumps.StatusText;

                if (prntVersion === false) {
                    CalculateHeightOnEachCDRow();

                    SectionHeader(CrashDumps.CrashDumpRecords.length, "CD");
                } else {
                    SectionHeader(0, "CD");
                }
            } else {
                SectionHeader(0, "CD");
            }
        }

        function ReportSectionsFadeIn(RptSection) {
            document.getElementById(RptSection).style.opacity = '1'
            document.getElementById(RptSection).style.left = '0';
            document.getElementById(RptSection).style.top = '0';
            document.getElementById(RptSection).style.webkitFilter = 'blur(0)';
        }

        function CalculateHeightOnEachWURow() {
            var rowHeight = 0;
            var intHeight = 0;

            if (WinUpdates.WURecords.length > 0) {
                for (i = 0; i < WinUpdates.WURecords.length; i++) {
                    intHeight = 0;
                    intHeight = document.getElementById('WU_EC' + i).clientHeight + 'px';
                    document.getElementById('WU_EC' + i).setAttribute('rowHeight', intHeight);
                    document.getElementById('WU_EC' + i).className = 'ExtraContent';
                }

                for (i = 0; i < WinUpdates.WURecords.length; i++) {
                    intHeight = 0;
                    intHeight = document.getElementById('WU_RHShown' + i).clientHeight; //This height check is done here because it is the cell that can make the whole row grow.
                    intHeight += 11;
                    rowHeight += intHeight;
                    document.getElementById('WU_RH' + i).style.height = intHeight;
                    document.getElementById('WU_RH' + i).setAttribute('rowHeight', intHeight);
                }

                document.getElementById('WinUpdBodyGoesHere').setAttribute('rowHeight', rowHeight);
            }
        }

        function SetRowHeaderforContentWUMouseOver(i) {
            var RowHeader = document.getElementById('WU_RH' + i);
            var ExtraContent = document.getElementById('WU_EC' + i);
            var SBLink = document.getElementById('SBLink' + i);
            var KBAIDLink = document.getElementById('KBAIDLink' + i);

            ExtraContent.removeAttribute('style');
            RowHeader.style.backgroundColor = 'rgb(83,84,85)';
            RowHeader.style.color = 'rgb(249,249,249)';
            ExtraContent.style.height = ExtraContent.getAttribute('rowheight');
            ExtraContent.style.padding = '5px;';

            if (SBLink != null) {
                SBLink.style.color = 'rgb(249,249,249)';
            }

            KBAIDLink.style.color = 'rgb(249,249,249)';
        }

        function SetRowHeaderforContentWUMouseOut(i) {
            var RowFormat = document.getElementById('WU_RF' + i);
            var RowHeader = document.getElementById('WU_RH' + i);
            var ExtraContent = document.getElementById('WU_EC' + i);
            var SBLink = document.getElementById('SBLink' + i);
            var KBAIDLink = document.getElementById('KBAIDLink' + i);

            RowHeader.style.backgroundColor = 'white';
            RowHeader.style.color = 'rgb(83,84,85)';
            ExtraContent.style.padding = '5px';
            ExtraContent.removeAttribute('style');
            RowFormat.style.WebkitTransitionDelay = '0.5s';
            RowFormat.style.transitionDelay = '0.5s';
            RowFormat.removeAttribute('style');

            if (SBLink != null) {
                SBLink.style.color = 'blue';
            }

            KBAIDLink.style.color = 'blue';
        }

        function WUExpandCollapseButton() {
            var Section = 'WinUpdBodyGoesHere';
            var i = 0;
            var rowHeight = 0;
            var WUHeight = 0;
            var RptExpandCollapse = document.getElementById('WUExpandCollapse');


            WUHeight = document.getElementById('WU_RowHeaders').clientHeight + 20; //Plus 10 for padding top and 10 for padding bottom

            for (i = 0; i < WinUpdates.WURecords.length; i++) {
                rowHeight = 0;
                rowHeight = document.getElementById('WU_RHShown' + i).clientHeight; //This height check is done here because it is the cell that can make the whole row grow.
                //WUHeight += rowHeight + 11;  //The extra 10 is 5px padding for bottom and top which is set in the "RowHeader" css class.
                //WUHeight += Number(document.getElementById('WU_RH' + i).getAttribute('rowHeight'));
                document.getElementById('WU_RH' + i).style.height = document.getElementById('WU_RH' + i).getAttribute('rowHeight') + 'px';
            }

            WUHeight = Number(document.getElementById(Section).getAttribute('rowHeight')) + 57 + 10 + 44 + 17;  //The 57 includes the header row below the message.  The 10 is for the last border line before the footer.  The 44 is the entire footer height.

            if (RptExpandCollapse.innerHTML == 'Show&nbsp;&nbsp;') {
                document.getElementById(Section).style.height = WUHeight + 'px';
                document.getElementById(Section).style.padding = '10px';
                setTimeout(function () { RptExpandCollapse.innerHTML = 'Hide&nbsp;&nbsp;'; }, 0900);
                setTimeout(function () { document.getElementById('WUExpandCollapsePic').src = 'data:image/png;base64,'; }, 0900);

                setTimeout(function () { RemoveStyleAttrFromDiv(Section) }, 0800);
            } else {
                document.getElementById(Section).style.height = WUHeight + 'px';
                setTimeout(function () { document.getElementById(Section).style.padding = '0px'; }, 0100)
                setTimeout(function () { document.getElementById(Section).style.overflow = 'hidden'; }, 0100)
                setTimeout(function () { document.getElementById(Section).style.height = '0px'; }, 0100)

                setTimeout(function () { WUSetHeightAttr(Section); }, 0800);
            }
        }

        function WUSetHeightAttr(DivName) {
            //document.getElementById(DivName).style.padding = '0px';
            //document.getElementById(DivName).style.overflow = 'hidden';
            //document.getElementById(DivName).style.height = '0px';

            setTimeout(function () { document.getElementById('WUExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;'; }, 0900);
            setTimeout(function () { document.getElementById('WUExpandCollapsePic').src = 'data:image/png;base64,'; }, 0900);
        }

        function RemoveStyleAttrFromDiv(DivName) {
            document.getElementById(DivName).removeAttribute('style');
            document.getElementById(DivName).style.padding = '10px';
        }

        function CalculateHeightOnEachBIOSRow() {
            var rowHeight = 0;
            var intHeight = 0;

            for (i = 0; i < BIOSSettingsData.BIOSSettingsRecords.length; i++) {
                if (BIOSSettingsData.BIOSSettingsRecords[i].Name != ' ' && BIOSSettingsData.BIOSSettingsRecords[i].Name != '') {
                    if (document.getElementById('BIOS_RH' + i) != null) {
                        intHeight = 0;
                        intHeight += document.getElementById('BIOS_RH' + i).clientHeight;
                        rowHeight += intHeight;
                        document.getElementById('BIOS_RH' + i).setAttribute('rowHeight', intHeight);
                    }
                }
            }

            document.getElementById('BIOSBodyGoesHere').setAttribute('rowHeight', rowHeight);
        }

        function CalculateHeightOnEachEngRow() {
            var rowHeight = 0;
            var intHeight = 0;

            for (i = 0; i < Eng.EngRecords.length; i++) {
                if (Eng.EngRecords[i].Name != ' ' && Eng.EngRecords[i].Name != '') {
                    if (document.getElementById('Eng_RH' + i) != null) {
                        intHeight = 0;
                        intHeight += document.getElementById('Eng_RH' + i).clientHeight;
                        rowHeight += intHeight;
                        document.getElementById('Eng_RH' + i).setAttribute('rowHeight', intHeight);
                    }
                }
            }

            document.getElementById('EngBodyGoesHere').setAttribute('rowHeight', rowHeight);
        }

        function BIOSExpandCollapseButton() {
            var Section = 'BIOSBodyGoesHere';
            var i = 0;
            var RowCnt = 0;
            var rowHeight = 0;
            var BIOSHeight = 0;
            var RptExpandCollapse = document.getElementById('BIOSExpandCollapse')
            
            if (prntVersion === false) {
                BIOSHeight = document.getElementById('BIOS_RowHeaders').clientHeight + 20; //Plus 10 for padding top and 10 for padding bottom

                for (i = 0; i < BIOSSettingsData.BIOSSettingsRecords.length; i++) {
                    if (BIOSSettingsData.BIOSSettingsRecords[i].Name != ' ' && BIOSSettingsData.BIOSSettingsRecords[i].Name != '') {
                        rowHeight = 0;
                        rowHeight = document.getElementById('BIOS_RHShown' + RowCnt).clientHeight; //This height check is done here because it is the cell that can make the whole row grow.
                        //BIOSHeight += rowHeight + 11;  //The extra 10 is 5px padding for bottom and top which is set in the "RowHeader" css class.
                        //BIOSHeight += Number(document.getElementById('BIOS_RH' + i).getAttribute('rowHeight'));
                        document.getElementById('BIOS_RH' + RowCnt).style.height = document.getElementById('BIOS_RH' + RowCnt).getAttribute('rowHeight') + 'px';

                        RowCnt++;
                    }
                }

                BIOSHeight = Number(document.getElementById(Section).getAttribute('rowHeight')) + 57 + 10 + 44 + 17;  //The 57 includes the header row below the message.  The 10 is for the last border line before the footer.  The 44 is the entire footer height.

                if (RptExpandCollapse.innerHTML == 'Show&nbsp;&nbsp;') {
                    document.getElementById(Section).style.height = BIOSHeight + 'px';
                    document.getElementById(Section).style.padding = '10px';
                    setTimeout(function () { RptExpandCollapse.innerHTML = 'Hide&nbsp;&nbsp;'; }, 1200);
                    setTimeout(function () { document.getElementById('BIOSExpandCollapsePic').src = 'data:image/png;base64,'; }, 1200);

                    setTimeout(function () { RemoveStyleAttrFromDiv(Section) }, 1300);
                } else {
                    document.getElementById(Section).style.height = BIOSHeight + 'px';
                    setTimeout(function () { BIOSSetHeightAttr(Section) }, 0100);
                }
            } else {
                document.getElementById(Section).style.height = 'inherit';
                document.getElementById(Section).style.padding = '0';
            }
        }

        function BIOSSetHeightAttr(DivName) {
            document.getElementById(DivName).style.padding = '0px';
            document.getElementById(DivName).style.overflow = 'hidden';
            document.getElementById(DivName).style.height = '0px';

            setTimeout(function () { document.getElementById('BIOSExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;'; }, 1200);
            setTimeout(function () { document.getElementById('BIOSExpandCollapsePic').src = 'data:image/png;base64,'; }, 1200);
        }

        function EngExpandCollapseButton() {
            var Section = 'EngBodyGoesHere';
            var i = 0;
            var rowHeight = 0;
            var EngHeight = 0;
            var RptExpandCollapse = document.getElementById('EngExpandCollapse');

            var Categories = [];
            while (Eng.EngRecords[i]) {
                EngCategory = Eng.EngRecords[i].Category == 'Null' ? '&nbsp;' : Eng.EngRecords[i].Category;

                bolFound = false;
                for (b = 0; b < Categories.length; b++) {
                    if (EngCategory == Categories[b]) {
                        bolFound = true;
                    }
                }

                if (bolFound == false)
                    Categories.push(EngCategory);

                i++;
            }

            if (Categories.length > 0)
                EngHeight = document.getElementById('Eng_RowHeaders0').clientHeight * Categories.length; //Plus 10 for padding top and 10 for padding bottom

            for (i = 0; i < Eng.EngRecords.length; i++) {
                rowHeight = 0;
                rowHeight = document.getElementById('Eng_RH' + i).clientHeight; //This height check is done here because it is the cell that can make the whole row grow.
                //EngHeight += rowHeight + 11;  //The extra 10 is 5px padding for bottom and top which is set in the "RowHeader" css class.
                document.getElementById('Eng_RH' + i).style.height = document.getElementById('Eng_RH' + i).getAttribute('rowHeight') + 'px';
            }

            EngHeight = Number(document.getElementById(Section).getAttribute('rowHeight')) + 57 + 10 + 44 + 195;  //The 57 includes the header row below the message.  The 10 is for the last border line before the footer.  The 44 is the entire footer height.

            if (RptExpandCollapse.innerHTML == 'Show&nbsp;&nbsp;') {
                document.getElementById(Section).style.height = EngHeight + 'px';
                document.getElementById(Section).style.padding = '10px';
                setTimeout(function () { RptExpandCollapse.innerHTML = 'Hide&nbsp;&nbsp;'; }, 0800);
                setTimeout(function () { document.getElementById('EngExpandCollapsePic').src = 'data:image/png;base64,'; }, 0800);
                setTimeout(function () { RemoveStyleAttrFromDiv(Section) }, 0800);
            } else {
                document.getElementById(Section).style.height = EngHeight + 'px';
                setTimeout(function () { EngSetHeightAttr(Section) }, 0100);
            }
        }

        function EngSetHeightAttr(DivName) {
            document.getElementById(DivName).style.padding = '0px';
            document.getElementById(DivName).style.overflow = 'hidden';
            document.getElementById(DivName).style.height = '0px';

            setTimeout(function () { document.getElementById('EngExpandCollapse').innerHTML = 'Show&nbsp;&nbsp;'; }, 0800);
            setTimeout(function () { document.getElementById('EngExpandCollapsePic').src = 'data:image/png;base64,'; }, 0800);
        }

        function CalculateHeightOnEachCDRow() {
            var rowHeight = 0;
            var intHeight = 0;

            for (i = 0; i < CrashDumps.CrashDumpRecords.length; i++) {
                if (CrashDumps.CrashDumpRecords[i].Name != ' ' && CrashDumps.CrashDumpRecords[i].Name != '') {
